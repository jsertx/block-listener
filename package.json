{
	"name": "whale",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Sergio",
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"start": "node build/index.js",
		"start:dev": "node --trace-warnings -r ts-node/register --inspect=9229 src/index.ts",
		"start:dev:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn start:dev",
		"test:dev": "node -r ts-node/register --inspect=9229 src/test.ts",
		"test:dev:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn test:dev",
		"build:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn build",
		"logs:prod": "heroku logs --tail -a blocklistener",
		"deploy:prod": "git push heroku main",
		"format": "prettier --write 'src/**/*.{ts,js}'"
	},
	"lint-staged": {
		"*.{ts,js}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@maticnetwork/eth-decoder": "git+https://github.com/jsertx/eth-decoder.git",
		"amqplib": "0.5.5",
		"axios": "^0.27.2",
		"bignumber.js": "^9.0.2",
		"body-parser": "^1.20.0",
		"dotenv": "^16.0.1",
		"ethers": "^5.6.5",
		"eventemitter3": "^4.0.7",
		"express": "^4.18.1",
		"inversify": "^6.0.1",
		"inversify-express-utils": "^6.4.3",
		"joi": "^17.6.0",
		"mongodb": "^4.6.0",
		"node-cron": "^3.0.1",
		"promise-throttle": "^1.1.2",
		"rascal": "^13.1.3",
		"reflect-metadata": "^0.1.13",
		"swagger-express-ts": "^1.1.0",
		"swagger-ui-dist": "^4.12.0",
		"type-fest": "^2.12.2",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.32",
		"@types/node-cron": "^3.0.2",
		"@types/rascal": "10.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.16",
		"prettier": "^2.7.1",
		"ts-node": "^10.8.0",
		"typescript": "^4.6.4"
	}
}
