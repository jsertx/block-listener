{
	"name": "whale",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Sergio",
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"start": "node build/index.js",
		"start:playground": "node --trace-warnings -r ts-node/register --inspect=9229",
		"start:dev": "node --trace-warnings -r ts-node/register --inspect=9229 src/all_in_one.ts",
		"start:dev:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn start:dev",
		"test:dev": "node -r ts-node/register --inspect=9229 src/test.ts",
		"test:dev:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn test:dev",
		"build:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn build",
		"format": "prettier --write 'src/**/*.{ts,js}'",
		"lint": "eslint . --ext .ts",
		"lint-and-fix": "eslint . --ext .ts --fix",
		"db:migration:create": "migrate-mongo create",
		"db:migration:up": "migrate-mongo up",
		"db:migration:down": "migrate-mongo down",
		"db:migration:status": "migrate-mongo status",
		"test": "jest"
	},
	"lint-staged": {
		"*.{ts,js}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@maticnetwork/eth-decoder": "git+https://github.com/jsertx/eth-decoder.git",
		"amqplib": "0.5.5",
		"axios": "^0.27.2",
		"bignumber.js": "^9.0.2",
		"body-parser": "^1.20.0",
		"bottleneck": "^2.19.5",
		"cheerio": "^1.0.0-rc.12",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.2",
		"ethereum-multicall": "^2.14.0",
		"ethers": "^5.6.5",
		"eventemitter3": "^4.0.7",
		"express": "^4.18.1",
		"inversify": "^6.0.1",
		"inversify-express-utils": "^6.4.3",
		"joi": "^17.6.0",
		"migrate-mongo": "^9.0.0",
		"mongodb": "^4.6.0",
		"node-cache": "^5.1.2",
		"node-cron": "^3.0.1",
		"promise-throttle": "^1.1.2",
		"rascal": "^13.1.3",
		"redis": "^3.1.2",
		"reflect-metadata": "^0.1.13",
		"type-fest": "^2.12.2",
		"uuid": "^8.3.2",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@logtail/types": "^0.1.12",
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^28.1.8",
		"@types/node": "^17.0.32",
		"@types/node-cron": "^3.0.2",
		"@types/rascal": "10.0.4",
		"@types/redis": "^2.8.32",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-no-floating-promise": "^1.0.2",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.1",
		"jest": "^28.0.8",
		"jest-mock-extended": "^2.0.7",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.16",
		"prettier": "^2.7.1",
		"ts-jest": "^28.0.8",
		"ts-node": "^10.8.0",
		"typescript": "^4.6.4"
	}
}
